
<div class="container">
  <div class="row col-md-4 col-md-offset-4" style="left: 0%; top: 70px;">
    <h3>Users</h3>
    <div class="column">
      <table class="table"  border="4">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

